{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "simple-card": {
      "projectType": "library",
      "root": "projects/simple-card",
      "sourceRoot": "projects/simple-card/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/simple-card/tsconfig.lib.json",
            "project": "projects/simple-card/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/simple-card/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/simple-card/src/test.ts",
            "tsConfig": "projects/simple-card/tsconfig.spec.json",
            "karmaConfig": "projects/simple-card/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/simple-card/tsconfig.lib.json",
              "projects/simple-card/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "test-components": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/test-components",
      "sourceRoot": "projects/test-components/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-components",
            "index": "projects/test-components/src/index.html",
            "main": "projects/test-components/src/main.ts",
            "polyfills": "projects/test-components/src/polyfills.ts",
            "tsConfig": "projects/test-components/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/test-components/src/favicon.ico",
              "projects/test-components/src/assets"
            ],
            "styles": [
              "projects/test-components/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/test-components/src/environments/environment.ts",
                  "with": "projects/test-components/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test-components:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "test-components:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-components:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test-components/src/test.ts",
            "polyfills": "projects/test-components/src/polyfills.ts",
            "tsConfig": "projects/test-components/tsconfig.spec.json",
            "karmaConfig": "projects/test-components/karma.conf.js",
            "assets": [
              "projects/test-components/src/favicon.ico",
              "projects/test-components/src/assets"
            ],
            "styles": [
              "projects/test-components/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/test-components/tsconfig.app.json",
              "projects/test-components/tsconfig.spec.json",
              "projects/test-components/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/test-components/e2e/protractor.conf.js",
            "devServerTarget": "test-components:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-components:serve:production"
            }
          }
        }
      }
    },
    "odprz-ui": {
      "projectType": "library",
      "root": "projects/odprz-ui",
      "sourceRoot": "projects/odprz-ui/src",
      "prefix": "odprz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/odprz-ui/tsconfig.lib.json",
            "project": "projects/odprz-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/odprz-ui/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/odprz-ui/src/test.ts",
            "tsConfig": "projects/odprz-ui/tsconfig.spec.json",
            "karmaConfig": "projects/odprz-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/odprz-ui/tsconfig.lib.json",
              "projects/odprz-ui/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "odprz-ui"
}
